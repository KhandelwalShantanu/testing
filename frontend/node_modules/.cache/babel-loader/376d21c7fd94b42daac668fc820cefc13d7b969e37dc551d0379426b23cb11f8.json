{"ast":null,"code":"import axios from 'axios';\nimport { timestamp } from 'public/assets/vendor/tui-calendar/tui-code-snippet/src/js/tricks';\nexport default {\n  data() {\n    return {\n      name: '',\n      email: '',\n      role: '',\n      contact_no: '',\n      designation: '',\n      pincode: '',\n      isToggled: false,\n      isAreaModal: false,\n      displayToggle: 'none',\n      users: [],\n      headers: ['Profile', 'Name', 'Designation', 'Role', 'Contact No.', 'Pincode', 'Actions']\n    };\n  },\n  methods: {\n    openModal() {\n      this.displayToggle = 'block';\n      this.isToggled = !this.isToggled;\n    },\n    createUser() {\n      const requestData = {\n        username: this.email,\n        name: this.name,\n        role: this.role,\n        contact_no: this.contact_no\n      };\n      axios.post('http://127.0.0.1:8000/api/create/user/');\n    },\n    closeModal() {\n      this.displayToggle = 'none';\n      this.isToggled = !this.isToggled;\n    },\n    getUsers(role) {\n      console.log(role);\n      axios.get('http://127.0.0.1:8000/api/users/', {\n        params: {\n          role: role\n        }\n      }).then(r => {\n        this.users = r.data.users;\n      });\n    }\n  },\n  watch: {\n    '$route.params.val': 'getUsers'\n  },\n  created() {\n    this.getUsers(this.$route.params.val);\n  }\n};","map":{"version":3,"names":["axios","timestamp","data","name","email","role","contact_no","designation","pincode","isToggled","isAreaModal","displayToggle","users","headers","methods","openModal","createUser","requestData","username","post","closeModal","getUsers","console","log","get","params","then","r","watch","created","$route","val"],"sources":["C:\\adsgrill\\frontend\\src\\components\\getEmployees.vue"],"sourcesContent":["<template>\r\n    <head>\r\n        <meta charset=\"utf-8\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, shrink-to-fit=no\">\r\n        <title>Webkit | Responsive Bootstrap 4 Admin D